<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.apifutbol">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ApiFutbol">
        <activity
            android:name=".Activity.EventPastMatch"
            android:exported="false" />
        <activity
            android:name=".Activity.AllLeagues"
            android:exported="false" />
        <activity
            android:name=".Activity.DetailTeam"
            android:exported="false" />
        <activity
            android:name=".Activity.MainActivity2"
            android:exported="false" />
        <activity
            android:name=".Activity.item_teams_by_league"
            android:exported="false" />
        <activity
            android:name=".Activity.AllTeamsByLeague"
            android:exported="false" />
        <!--
        android:requestLegacyExternalStorage="true"
        android:hardwareAccelerated="false"
        android:largeHeap="true"
        -->
        <activity
            android:name=".Activity.DetailLeague"
            android:exported="false" />
        <activity
            android:name=".Activity.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>